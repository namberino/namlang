lớp Node
{
    tạo(dữ_liệu) {
        đây.dữ_liệu = dữ_liệu;
        đây.tiếp = không;
    }
}

lớp LinkedList {
    tạo() {
        đây.đầu = không;
    }

    in_list() {
        biến giá_trị = đây.đầu;

        khi (giá_trị != không) {
            in(giá_trị.dữ_liệu);
            giá_trị = giá_trị.tiếp;
        }
    }
}

biến ll = LinkedList();
ll.đầu = Node(1);
biến second = Node(2);
biến third = Node(3);

ll.đầu.tiếp = second;
second.tiếp = third;
ll.in_list();
