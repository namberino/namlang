biến tìm_kiếm = [];


cho (biến i = 0; i < 100; i = i + 1)
{
    tìm_kiếm[i] = không;
}

hàm dynamic_fib(n)
{
    nếu (n == 0 hoặc n == 1)
    {
        trả 1;
    }

    nếu (tìm_kiếm[n] != không)
    {
        trả tìm_kiếm[n];
    }

    tìm_kiếm[n] = dynamic_fib(n - 1) + dynamic_fib(n - 2);

    trả tìm_kiếm[n];
}

in(dynamic_fib(5));
